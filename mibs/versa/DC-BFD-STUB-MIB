--       bfdSessReqMinEchoRxInterval,
--       bfdSessDetectMult,
--       bfdSessStorType,
--       bfdSessRowStatus,
-- Begin Versa change {
--       dcBfdSessMapBfdIndex,
-- End Versa change }
         dcBfdSessConfigRowStatus,
