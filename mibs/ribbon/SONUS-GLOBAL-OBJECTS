        sonusGlobalTcpGeneralGroupStatisticsEstablishedConnections Gauge32,
        sonusGlobalTcpGeneralGroupStatisticsClientConnections Counter32,
        sonusGlobalTcpGeneralGroupStatisticsServerConnections Counter32,
        sonusGlobalTcpGeneralGroupStatisticsFailedAttempts Counter32,
        sonusGlobalTcpGeneralGroupStatisticsInSegments Counter32,
        sonusGlobalTcpGeneralGroupStatisticsOutSegments Counter32,
        sonusGlobalTcpGeneralGroupStatisticsRetransSegments Counter32,
        sonusGlobalTcpGeneralGroupStatisticsInErrors Counter32,
        sonusGlobalTcpGeneralGroupStatisticsInResets Counter32,
        sonusGlobalTcpGeneralGroupStatisticsOutResets Counter32
    }

