    ::= { sonusGlobalMIB 339 }

-- tagpath /global/callCountIntervalStatistics
sonusGlobalCallCountIntervalStatisticsEntry OBJECT-TYPE
    SYNTAX      SonusGlobalCallCountIntervalStatisticsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    INDEX { sonusGlobalCallCountIntervalStatisticsNumber, sonusGlobalCallCountIntervalStatisticsName }
        ::= { sonusGlobalCallCountIntervalStatisticsTable 1 }

SonusGlobalCallCountIntervalStatisticsEntry ::=
    SEQUENCE {
        sonusGlobalCallCountIntervalStatisticsNumber Unsigned32,
        sonusGlobalCallCountIntervalStatisticsName INTEGER,
        sonusGlobalCallCountIntervalStatisticsIntervalValid INTEGER,
        sonusGlobalCallCountIntervalStatisticsTime Unsigned32,
        sonusGlobalCallCountIntervalStatisticsCallCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsEncryptCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsSrtpCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsEnhancedVideoCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsAmrnbLegCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsAmrwbLegCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsEvrcLegCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsNiceRecCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsMrfSessionsCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsSipRecCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsEvsLegCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsTranscodeCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsPdcsCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsSilkLegCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsLicenseMode String,
        sonusGlobalCallCountIntervalStatisticsSlbSessionsCount Unsigned32,
        sonusGlobalCallCountIntervalStatisticsHpcQueueAttempts Unsigned32,
        sonusGlobalCallCountIntervalStatisticsHpcQueueOverflows Unsigned32,
        sonusGlobalCallCountIntervalStatisticsHpcQueueAbandons Unsigned32,
        sonusGlobalCallCountIntervalStatisticsHpcQueueTimeouts Unsigned32,
        sonusGlobalCallCountIntervalStatisticsHpcTrafficControlExemptions Unsigned32
    }

-- tagpath /global/callCountIntervalStatistics/number
sonusGlobalCallCountIntervalStatisticsNumber OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "
A sequence number which identifies the
interval for which the set of statistics is required.
The interval N identifies the recently completed interval,
Interval N is identifed by the last sequence value and N-( number of intervals)
is the earlest completed interval.
"
    ::= { sonusGlobalCallCountIntervalStatisticsEntry 1 }

-- tagpath /global/callCountIntervalStatistics/name
sonusGlobalCallCountIntervalStatisticsName OBJECT-TYPE
    SYNTAX      INTEGER {entry(1)}
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "
A dummy key to be added to non-config scalar element container, even though only
one row is allowed in container. It is because without the key, tailf treats the
container non-dynamic and calls get_elem per leaf instead of calling get_object per row
"
    ::= { sonusGlobalCallCountIntervalStatisticsEntry 2 }

-- tagpath /global/callCountIntervalStatistics/intervalValid
sonusGlobalCallCountIntervalStatisticsIntervalValid OBJECT-TYPE
    SYNTAX      INTEGER {true(1),false(2)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The member indicating the validity of the interval.
"
    ::= { sonusGlobalCallCountIntervalStatisticsEntry 3 }

