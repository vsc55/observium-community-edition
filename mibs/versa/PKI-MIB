PKI-MIB DEFINITIONS ::= BEGIN
IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE,
    Integer32, Unsigned32, Counter32, Counter64,
    Gauge32, IpAddress
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION, RowStatus, DateAndTime,
    TruthValue
        FROM SNMPv2-TC
    orgServices
        FROM VERSA-MIB
;

pki MODULE-IDENTITY
    LAST-UPDATED "201604140000Z"
    ORGANIZATION "Versa Networks"
    CONTACT-INFO "
        2953 Bunker Hill Lane, Suite 210
        Santa Clara, CA - 95035 (USA)
        Email: support@versa-networks.com"
    DESCRIPTION "Enterprise MIB for Versa Networks'
Crypto stats"
    REVISION "201604140000Z"
    DESCRIPTION "Initial revision"
    ::= { orgServices 13 }

String ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "1t"
    STATUS      current
    DESCRIPTION "xs:string"
    SYNTAX      OCTET STRING

certCsrTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF CertCsrEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Display information about a cert csr stats"
    ::= { pki 1 }

certCsrEntry OBJECT-TYPE
    SYNTAX      CertCsrEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    INDEX { certProfileOrgId, certCsrId }
        ::= { certCsrTable 1 }

CertCsrEntry ::=
    SEQUENCE {
        certProfileOrgId Unsigned32,
        certCsrId Unsigned32,
        certProfileOrgName String,
        certCsrName String,
        certCsrIrCertCheckSuccess Unsigned32,
        certCsrIrCertCheckFail Unsigned32,
        certCsrIrIntfNotUp Unsigned32,
        certCsrIrIntfAddrUnavail Unsigned32,
        certCsrIrCertAuthKeyFail Unsigned32,
        certCsrIrCertAuthCertFail Unsigned32,
        certCsrIrCertKeyGenFail Unsigned32,
        certCsrIrCertReqGenFail Unsigned32,
        certCsrIrCertEnrollSent Unsigned32,
        certCsrIrCertSwitchNsFail Unsigned32,
        certCsrIrCertRevertNsFail Unsigned32,
        certCsrIrCertSignSuccess Unsigned32,
        certCsrIrCaCertSignSuccess Unsigned32,
        certCsrIrCertSignRejected Unsigned32,
        certCsrIrCertSignDone Unsigned32,
        certCsrIrCertSignFailure Unsigned32,
        certCsrKurCertCheckSuccess Unsigned32,
        certCsrKurCertCheckFail Unsigned32,
        certCsrKurIntfNotUp Unsigned32,
        certCsrKurIntfAddrUnavail Unsigned32,
        certCsrKurCertAuthKeyFail Unsigned32,
        certCsrKurCertAuthCertFail Unsigned32,
        certCsrKurCertKeyGenFail Unsigned32,
        certCsrKurCertReqGenFail Unsigned32,
        certCsrKurCertEnrollSent Unsigned32,
        certCsrKurCertSwitchNsFail Unsigned32,
        certCsrKurCertRevertNsFail Unsigned32,
        certCsrKurCertSignSuccess Unsigned32,
        certCsrKurCaCertSignSuccess Unsigned32,
        certCsrKurCertSignRejected Unsigned32,
        certCsrKurCertSignDone Unsigned32,
        certCsrKurCertSignFailure Unsigned32,
        certCsrIrRtInstUnavail Unsigned32,
        certCsrIrRrIntfUnavail Unsigned32,
        certCsrKurRtInstUnavail Unsigned32,
        certCsrKurRrIntfUnavail Unsigned32
    }

