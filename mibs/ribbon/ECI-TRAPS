	::= { eciTrapObjects 34 }

  eciRedundantTimingPair    OBJECT-TYPE
	SYNTAX		INTEGER { ctm0 (0), ctm1 (1) }
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
