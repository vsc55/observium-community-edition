    optixCurtPingtime OBJECT-TYPE
        SYNTAX INTEGER
        {
            twoSecond(0),
            tenSecond(1)
        }
        MAX-ACCESS read-create
        STATUS current
        DESCRIPTION
            "This entry decribes the timeout value,range 0~1,default 0,0 means 1sec,1 means 10sec"
        ::= { optixPKTIPPingStateEntry 8 }

