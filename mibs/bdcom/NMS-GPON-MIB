                     onuTypeTemplateWanPortConfigPppoePortIndex 
                         INTEGER,
                     onuTypeTemplateWanPortConfigPppoeUsername
                         OCTET STRING,
                     onuTypeTemplateWanPortConfigPppoePassword
                         OCTET STRING,
                     onuTypeTemplateWanPortConfigPppoeRowStatus
                         RowStatus               
                 }

