                        INTEGER,
                    onuVoipConfigProfileVoiceSvcHookFlashMinTime
                        INTEGER,
                    onuVoipConfigProfileVoiceSvcHookFlashMaxTime
                        INTEGER
                }

          onuVoipConfigProfileIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
