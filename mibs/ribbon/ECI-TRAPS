	::= { eciFeTrapsZero 6 }

    feTurnTransmitterOffDetected NOTIFICATION-TYPE
        OBJECTS { ifIndex, ifName }
	STATUS deprecated
	DESCRIPTION
	    "This trap indicates that a request has come down from
	    a higher layer, such as an l2t to disable the transmitter"
	REFERENCE
