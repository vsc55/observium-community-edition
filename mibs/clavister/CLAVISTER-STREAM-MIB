SsmMemCatEntry ::= SEQUENCE {
    ssmMemCatIndex                 Integer32,
    ssmMemCatName                  DisplayString,
    ssmMemCatCtrlPlaneUsage        CounterBasedGauge64,
    ssmMemCatDataPlaneUsage        CounterBasedGauge64
}

