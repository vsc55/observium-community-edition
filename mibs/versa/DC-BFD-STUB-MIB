         bfdDisableLibrary,
         bfdVersionNumber,
         bfdVrfName,
--       bfdSessApplicationId,
         bfdSessDiscriminator,
         bfdSessIntface,
         bfdSessAddrType,
         bfdSessAddr,
         bfdSessApplicationSessions,
         bfdSessRemoteDiscr,
         bfdSessLocalAddrType,
         bfdSessLocalAddr,
         bfdSessDesiredMinTxIntvl,
         bfdSessReqMinRxInterval,
         bfdSessDetectMult,
--       bfdSessUdpPort,
         bfdSessState,
--       bfdSessRemoteHeardFlag,
         bfdSessDiag,
