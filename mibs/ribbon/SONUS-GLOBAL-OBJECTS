        sonusGlobalIpGeneralGroupStatisticsCeName String,
        sonusGlobalIpGeneralGroupStatisticsInReceives Counter32,
        sonusGlobalIpGeneralGroupStatisticsInHdrErrors Counter32,
        sonusGlobalIpGeneralGroupStatisticsInAddrErrors Counter32,
        sonusGlobalIpGeneralGroupStatisticsInUnknownProtos Counter32,
        sonusGlobalIpGeneralGroupStatisticsInDiscards Counter32,
        sonusGlobalIpGeneralGroupStatisticsInDelivers Counter32,
        sonusGlobalIpGeneralGroupStatisticsOutRequests Counter32,
        sonusGlobalIpGeneralGroupStatisticsOutDiscards Counter32,
        sonusGlobalIpGeneralGroupStatisticsOutNoRoutes Counter32,
        sonusGlobalIpGeneralGroupStatisticsReasmTimeout Integer32,
        sonusGlobalIpGeneralGroupStatisticsReasmReqds Counter32,
        sonusGlobalIpGeneralGroupStatisticsReasmOKs Counter32,
        sonusGlobalIpGeneralGroupStatisticsReasmFails Counter32,
        sonusGlobalIpGeneralGroupStatisticsFragOKs Counter32,
        sonusGlobalIpGeneralGroupStatisticsFragFails Counter32
    }

