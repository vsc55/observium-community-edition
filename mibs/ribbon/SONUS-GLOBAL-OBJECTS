-- tagpath /global/policyServer/lnpTfCounters
sonusGlobalPolicyServerLnpTfCountersTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SonusGlobalPolicyServerLnpTfCountersEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "
Policy LNP and Tollfree counters
"
    ::= { sonusGlobalMIB 16 }

-- tagpath /global/policyServer/lnpTfCounters
sonusGlobalPolicyServerLnpTfCountersEntry OBJECT-TYPE
    SYNTAX      SonusGlobalPolicyServerLnpTfCountersEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    INDEX { sonusGlobalPolicyServerLnpTfCountersSystemName }
        ::= { sonusGlobalPolicyServerLnpTfCountersTable 1 }

SonusGlobalPolicyServerLnpTfCountersEntry ::=
    SEQUENCE {
        sonusGlobalPolicyServerLnpTfCountersSystemName String,
        sonusGlobalPolicyServerLnpTfCountersNumLNPRequests Counter32,
        sonusGlobalPolicyServerLnpTfCountersNumSCPRequests Counter32,
        sonusGlobalPolicyServerLnpTfCountersNumDNRequests Counter32,
        sonusGlobalPolicyServerLnpTfCountersNumTimedOutRequests Counter32,
        sonusGlobalPolicyServerLnpTfCountersNumRequestErrors Counter32,
        sonusGlobalPolicyServerLnpTfCountersTfcNumRequests Counter32,
        sonusGlobalPolicyServerLnpTfCountersTfcNumRequestsToScp Counter32,
        sonusGlobalPolicyServerLnpTfCountersTfcNumRequestsTranslated Counter32,
        sonusGlobalPolicyServerLnpTfCountersTfcNumTimedOutRequests Counter32,
        sonusGlobalPolicyServerLnpTfCountersTfcNumRequestErrors Counter32
    }

-- tagpath /global/policyServer/lnpTfCounters/systemName
sonusGlobalPolicyServerLnpTfCountersSystemName OBJECT-TYPE
    SYNTAX      String (SIZE (0 .. 255))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    ::= { sonusGlobalPolicyServerLnpTfCountersEntry 1 }

-- tagpath /global/policyServer/lnpTfCounters/numLNPRequests
sonusGlobalPolicyServerLnpTfCountersNumLNPRequests OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
Number of requests that need LNP: Total
number of requests received by the Sonus
SoftSwitch which require LNP translation.
"
    ::= { sonusGlobalPolicyServerLnpTfCountersEntry 2 }

-- tagpath /global/policyServer/lnpTfCounters/numSCPRequests
sonusGlobalPolicyServerLnpTfCountersNumSCPRequests OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
Number of LNP requests sent to SCP: Total
number of requests sent to the SCP for LNP
translation.
"
    ::= { sonusGlobalPolicyServerLnpTfCountersEntry 3 }

