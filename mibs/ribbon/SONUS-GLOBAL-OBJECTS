-- tagpath /global/callRemoteMediaStatus
sonusGlobalCallRemoteMediaStatusEntry OBJECT-TYPE
    SYNTAX      SonusGlobalCallRemoteMediaStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    INDEX { sonusGlobalCallRemoteMediaStatusGCID, sonusGlobalCallRemoteMediaStatusResIndex }
        ::= { sonusGlobalCallRemoteMediaStatusTable 1 }

SonusGlobalCallRemoteMediaStatusEntry ::=
    SEQUENCE {
        sonusGlobalCallRemoteMediaStatusGCID Unsigned32,
        sonusGlobalCallRemoteMediaStatusResIndex Unsigned32,
        sonusGlobalCallRemoteMediaStatusStreamId Unsigned32,
        sonusGlobalCallRemoteMediaStatusResId Integer32,
        sonusGlobalCallRemoteMediaStatusResType SonusCallResTypeType,
        sonusGlobalCallRemoteMediaStatusLegId Integer32,
        sonusGlobalCallRemoteMediaStatusNodeGcidAndIpAddr String,
        sonusGlobalCallRemoteMediaStatusLocalRtpPort Unsigned32,
        sonusGlobalCallRemoteMediaStatusRemoteRtpPort Unsigned32,
        sonusGlobalCallRemoteMediaStatusRemoteRtcpPort Unsigned32,
        sonusGlobalCallRemoteMediaStatusRtpPacketSent SonusPacketCountType,
        sonusGlobalCallRemoteMediaStatusRtpPacketRecv SonusPacketCountType,
        sonusGlobalCallRemoteMediaStatusRtcpPacketSent SonusPacketCountType,
        sonusGlobalCallRemoteMediaStatusRtcpPacketRecv SonusPacketCountType,
        sonusGlobalCallRemoteMediaStatusRtpPacketDiscard SonusPacketCountType
    }

