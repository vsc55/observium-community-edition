              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory

              DESCRIPTION
                      ""
              ::= { onuCommonPortStateEntry 1 }


          onuCommonPortStatePortIndex OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory

              DESCRIPTION
                      "PortIndex consists of port_type and port_id:
                      PortIndex = (port_type << 8) | port_id
                      port_type define:
                      ethuni    0x00
                      veip      0x02
                      iphost    0x04 "
              ::= { onuCommonPortStateEntry 2 }

          onuCommonPortStateOperationalState OBJECT-TYPE   
              SYNTAX  INTEGER{
                               enable(1),
                               disable(2)
                             }
              ACCESS  read-only
              STATUS  mandatory

              DESCRIPTION
                      "1-enable 2-disable"
              ::= { onuCommonPortStateEntry 3 }

