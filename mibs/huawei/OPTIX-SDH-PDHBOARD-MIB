        SYNTAX INTEGER
        {
            xc(1),
            liu(2)
        }
        MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
            "This object indicates direction of the bit test. 1: xc, 2: liu"
        ::= { optixStartBitTestExtEntry 2 }

    -- 1.3.6.1.4.1.2011.2.25.4.10.44.1.8.1.3
