            optixStartBitTestExtAction
                INTEGER,
            optixStartBitTestExtDir
                INTEGER,
            optixStartBitTestExtMode
                Unsigned32,
            optixStartBitTestExtTime
                Unsigned32
        }

    -- 1.3.6.1.4.1.2011.2.25.4.10.44.1.8.1.1
