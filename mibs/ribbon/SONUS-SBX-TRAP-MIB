                	sonusAlarmNodeID,
				    sonusAlarmLevel,
				    sonusAlarmSequenceId,
                    sonusAlarmTime,
                    sonusAlarmDescription,
				    sonusTgName,
                    sonusTrunkGroupAutoCon
                   }
        STATUS       deprecated
        DESCRIPTION
                "This trap has been replaced by sonusSbxTrunkGroupManualControlActive3." 
        ::= { sonusTrunkGroupResourcesMIBNotificationsPrefix 11 }
                
                
