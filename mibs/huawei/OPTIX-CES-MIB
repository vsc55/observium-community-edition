        SYNTAX INTEGER
        {
            transitTunnel(0),
            msPw(1)
        }
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "This object indicates the MPLS type. 0:Transit Tunnel;1:MSPW"
        ::= { optixCesDropCheckEntry 1 }

    -- 1.3.6.1.4.1.2011.2.25.4.50.2.1.6.1.2
