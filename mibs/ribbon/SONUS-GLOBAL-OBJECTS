    ::= { sonusGlobalDtlsSrtpIntervalStatisticsEntry 9 }

-- tagpath /global/dtlsSrtpIntervalStatistics/handshakeTimeouts
sonusGlobalDtlsSrtpIntervalStatisticsHandshakeTimeouts OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
Number of handshakes that timed out before completing.
"
    ::= { sonusGlobalDtlsSrtpIntervalStatisticsEntry 10 }

