--
-- Traps for NIF
--

sonusCpNifMIBNotifications OBJECT IDENTIFIER ::= { sonusCpBaseTraps 6 }

sonusCpNifMIBNotificationsPrefix OBJECT IDENTIFIER ::= { sonusCpNifMIBNotifications 0 }
sonusCpNifMIBNotificationsObjects OBJECT IDENTIFIER ::= { sonusCpNifMIBNotifications 1 }


--
-- Need to add ceName, type, and usage to each NIF trap once the NIF MIB gets generated.
--

sonusCpNifCrcErrorRateExceededNotification NOTIFICATION-TYPE

	OBJECTS	{   
			    sonusAlarmNodeID,
				sonusAlarmLevel,
				sonusAlarmSequenceId,
                sonusAlarmTime,
                sonusAlarmDescription,
                sonusCpNifCeName,
                sonusCpNifType,
                sonusCpNifUsage
			}
	STATUS		current
	DESCRIPTION
		"This trap indicates that CRC error rate has exceeded set threshold for the set duration."
	::= { sonusCpNifMIBNotificationsPrefix 1 }

sonusCpNifCrcErrorRateClearedNotification NOTIFICATION-TYPE
	OBJECTS	{   
			    sonusAlarmNodeID,
				sonusAlarmLevel,
				sonusAlarmSequenceId,
                sonusAlarmTime,
                sonusAlarmDescription,
                sonusCpNifCeName,
                sonusCpNifType,
                sonusCpNifUsage
			}
	STATUS		current
	DESCRIPTION
		"This trap indicates that CRC error rate gone below the clear threshold for the clear duration."
	::= { sonusCpNifMIBNotificationsPrefix 2 }

