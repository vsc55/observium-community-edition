    SYNTAX      SEQUENCE OF SonusGlobalServiceAuthorisedIntStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "
Global interval service authorisation statistics.
"
    ::= { sonusGlobalMIB 342 }

-- tagpath /global/serviceAuthorisedIntStats
sonusGlobalServiceAuthorisedIntStatsEntry OBJECT-TYPE
    SYNTAX      SonusGlobalServiceAuthorisedIntStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    INDEX { sonusGlobalServiceAuthorisedIntStatsNumber, sonusGlobalServiceAuthorisedIntStatsName }
        ::= { sonusGlobalServiceAuthorisedIntStatsTable 1 }

SonusGlobalServiceAuthorisedIntStatsEntry ::=
    SEQUENCE {
        sonusGlobalServiceAuthorisedIntStatsNumber Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsName INTEGER,
        sonusGlobalServiceAuthorisedIntStatsIntervalValid INTEGER,
        sonusGlobalServiceAuthorisedIntStatsTime Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsLicenseMode SonusLicenseModeType,
        sonusGlobalServiceAuthorisedIntStatsEncryptAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsSrtpAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsEnhancedVideoAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsAmrnbLegAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsAmrwbLegAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsEvrcLegAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsNiceRecAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsMrfSessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsSipRecAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsTranscodeAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsPdcsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsLiSessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsSbcRtuSessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsDspG722SessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsGmp4x1SessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsSipISessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsSip323SessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsGmp1x10SessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsPolRtuSessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsPsxRtuSessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsCapacityLicenseAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsE911SessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsEnumSessionsAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsSwInstanceLicenseAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsEvsLegAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsSilkLegAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsSlbAuthorisation Unsigned32,
        sonusGlobalServiceAuthorisedIntStatsSlbSessionsAuthorisation Unsigned32
    }

-- tagpath /global/serviceAuthorisedIntStats/number
sonusGlobalServiceAuthorisedIntStatsNumber OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "
A sequence number which identifies the
interval for which the set of statistics is required.
The interval N identifies the recently completed interval,
Interval N is identifed by the last sequence value and N-( number of intervals)
is the earlest completed interval.
"
    ::= { sonusGlobalServiceAuthorisedIntStatsEntry 1 }

-- tagpath /global/serviceAuthorisedIntStats/name
sonusGlobalServiceAuthorisedIntStatsName OBJECT-TYPE
    SYNTAX      INTEGER {entry(1)}
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "
A dummy key to be added to non-config scalar element container, even though only
one row is allowed in container. It is because without the key, tailf treats the
container non-dynamic and calls get_elem per leaf instead of calling get_object per row
"
    ::= { sonusGlobalServiceAuthorisedIntStatsEntry 2 }

-- tagpath /global/serviceAuthorisedIntStats/intervalValid
sonusGlobalServiceAuthorisedIntStatsIntervalValid OBJECT-TYPE
    SYNTAX      INTEGER {true(1),false(2)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The member indicating the validity of the interval.
"
    ::= { sonusGlobalServiceAuthorisedIntStatsEntry 3 }

