deviceTotalPower OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "
			Total output power in Watts"
	::= { deviceEntry 7 }

devicePeakPowerLast24hrs OBJECT-TYPE
	SYNTAX Integer32
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION "
			Maximum output power over the past 24 hours in Watts"
	::= { deviceEntry 8 }

