--
-- @Description: MIB for Guest VNFs
-- @Date       : 2018-10-05
--

GUEST-VNF-TRAP DEFINITIONS ::= BEGIN
IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE
        FROM SNMPv2-SMI
    systemNotification
        FROM VERSA-MIB
    alarmIndex, alarmType, alarmDevice, alarmObject,
    alarmSpecificProblem, alarmEventType, alarmProbableCause,
    alarmTime, alarmSeverity, alarmHasClear, alarmText,
    tenantName, alarmClass
        FROM TRAPS-COMMON;

guestVnfTrapMIB MODULE-IDENTITY
    LAST-UPDATED "201706060000Z"
    ORGANIZATION "Versa Networks"
    CONTACT-INFO "
        2953 Bunker Hill Lane, Suite 210
        Santa Clara, CA - 95035 (USA)
        Email: support@versa-networks.com"
    DESCRIPTION  "Versa Networks' Entrprise MIB for Guest VNF Traps"
    ::= { systemNotification 6 }

-- Root Object
guestVnfEntities OBJECT IDENTIFIER ::= { guestVnfTrapMIB 1 }
guestVnfNotification OBJECT IDENTIFIER ::= { guestVnfTrapMIB 2 }

guestVnfName OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION "Guest VNF name"
        ::= { guestVnfEntities 1 }

guestVnfIp OBJECT-TYPE
    SYNTAX      OCTET STRING
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION "Guest VNF management IP address"
        ::= { guestVnfEntities 2 }

guestVnfDown NOTIFICATION-TYPE
    OBJECTS {
        alarmType, alarmDevice, alarmObject, alarmSpecificProblem,
        alarmClass, alarmEventType, alarmProbableCause, alarmTime,
        alarmSeverity, alarmHasClear, alarmText, tenantName,
        guestVnfName
    }
    STATUS  current
    DESCRIPTION "Monitor down notification"
        ::= { guestVnfNotification 1 }

guestVnfUp NOTIFICATION-TYPE
    OBJECTS {
        alarmType, alarmDevice, alarmObject, alarmSpecificProblem,
        alarmClass, alarmEventType, alarmProbableCause, alarmTime,
        alarmSeverity, alarmHasClear, alarmText, tenantName,
        guestVnfName
    }
    STATUS  current
    DESCRIPTION "Monitor up notification"
        ::= { guestVnfNotification 2 }

END
