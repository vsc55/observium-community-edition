    ::= { sonusGlobalMIB 40 }

-- tagpath /global/icmpGeneralGroupIntervalStatistics
sonusGlobalIcmpGeneralGroupIntervalStatisticsEntry OBJECT-TYPE
    SYNTAX      SonusGlobalIcmpGeneralGroupIntervalStatisticsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    INDEX { sonusGlobalIcmpGeneralGroupIntervalStatisticsCeName }
        ::= { sonusGlobalIcmpGeneralGroupIntervalStatisticsTable 1 }

SonusGlobalIcmpGeneralGroupIntervalStatisticsEntry ::=
    SEQUENCE {
        sonusGlobalIcmpGeneralGroupIntervalStatisticsCeName String,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsInMsgs Counter32,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsInEchos Counter32,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsInEchoReps Counter32,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsInDestUnreachs Counter32,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsInErrors Counter32,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsOutMsgs Counter32,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsOutErrors Counter32,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsOutEchos Counter32,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsOutEchoReps Counter32,
        sonusGlobalIcmpGeneralGroupIntervalStatisticsOutDestUnreachs Counter32
    }

-- tagpath /global/icmpGeneralGroupIntervalStatistics/ceName
sonusGlobalIcmpGeneralGroupIntervalStatisticsCeName OBJECT-TYPE
    SYNTAX      String (SIZE (0 .. 255))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "
The name of the compute element for this entry.
"
    ::= { sonusGlobalIcmpGeneralGroupIntervalStatisticsEntry 1 }

-- tagpath /global/icmpGeneralGroupIntervalStatistics/inMsgs
sonusGlobalIcmpGeneralGroupIntervalStatisticsInMsgs OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The total number of ICMP messages which the entity
received.  Note that this counter includes all those counted
by icmpInErrors.
"
    ::= { sonusGlobalIcmpGeneralGroupIntervalStatisticsEntry 2 }

-- tagpath /global/icmpGeneralGroupIntervalStatistics/inEchos
sonusGlobalIcmpGeneralGroupIntervalStatisticsInEchos OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The number of ICMP Echo (request) messages received.
"
    ::= { sonusGlobalIcmpGeneralGroupIntervalStatisticsEntry 3 }

-- tagpath /global/icmpGeneralGroupIntervalStatistics/inEchoReps
sonusGlobalIcmpGeneralGroupIntervalStatisticsInEchoReps OBJECT-TYPE
    SYNTAX      Counter32
