-- tagpath /global/tcpGeneralGroupCurrentStatistics
sonusGlobalTcpGeneralGroupCurrentStatisticsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SonusGlobalTcpGeneralGroupCurrentStatisticsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "
The collection of TCP General Group information for the
specified instance.
"
    ::= { sonusGlobalMIB 43 }

-- tagpath /global/tcpGeneralGroupCurrentStatistics
sonusGlobalTcpGeneralGroupCurrentStatisticsEntry OBJECT-TYPE
    SYNTAX      SonusGlobalTcpGeneralGroupCurrentStatisticsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    INDEX { sonusGlobalTcpGeneralGroupCurrentStatisticsCeName }
        ::= { sonusGlobalTcpGeneralGroupCurrentStatisticsTable 1 }

SonusGlobalTcpGeneralGroupCurrentStatisticsEntry ::=
    SEQUENCE {
        sonusGlobalTcpGeneralGroupCurrentStatisticsCeName String,
        sonusGlobalTcpGeneralGroupCurrentStatisticsEstablishedConnections Gauge32,
        sonusGlobalTcpGeneralGroupCurrentStatisticsClientConnections Counter32,
        sonusGlobalTcpGeneralGroupCurrentStatisticsServerConnections Counter32,
        sonusGlobalTcpGeneralGroupCurrentStatisticsFailedAttempts Counter32,
        sonusGlobalTcpGeneralGroupCurrentStatisticsInSegments Counter32,
        sonusGlobalTcpGeneralGroupCurrentStatisticsOutSegments Counter32,
        sonusGlobalTcpGeneralGroupCurrentStatisticsRetransSegments Counter32,
        sonusGlobalTcpGeneralGroupCurrentStatisticsInErrors Counter32,
        sonusGlobalTcpGeneralGroupCurrentStatisticsInResets Counter32,
        sonusGlobalTcpGeneralGroupCurrentStatisticsOutResets Counter32
    }

-- tagpath /global/tcpGeneralGroupCurrentStatistics/ceName
sonusGlobalTcpGeneralGroupCurrentStatisticsCeName OBJECT-TYPE
    SYNTAX      String (SIZE (0 .. 255))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "
The name of the compute element for this entry.
"
    ::= { sonusGlobalTcpGeneralGroupCurrentStatisticsEntry 1 }

-- tagpath /global/tcpGeneralGroupCurrentStatistics/establishedConnections
sonusGlobalTcpGeneralGroupCurrentStatisticsEstablishedConnections OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The number of TCP connections for which the current state
is either ESTABLISHED or CLOSE- WAIT.
(currEstab)
"
    ::= { sonusGlobalTcpGeneralGroupCurrentStatisticsEntry 2 }

-- tagpath /global/tcpGeneralGroupCurrentStatistics/clientConnections
sonusGlobalTcpGeneralGroupCurrentStatisticsClientConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The number of times TCP connections have made a direct
transition to the SYN-SENT state from the CLOSED state.
(activeOpens).
"
    ::= { sonusGlobalTcpGeneralGroupCurrentStatisticsEntry 3 }

-- tagpath /global/tcpGeneralGroupCurrentStatistics/serverConnections
sonusGlobalTcpGeneralGroupCurrentStatisticsServerConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The number of times TCP connections have made a direct
transition to the SYN-RCVD state from the LISTEN state.
(passiveOpens).
"
    ::= { sonusGlobalTcpGeneralGroupCurrentStatisticsEntry 4 }

-- tagpath /global/tcpGeneralGroupCurrentStatistics/failedAttempts
sonusGlobalTcpGeneralGroupCurrentStatisticsFailedAttempts OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The number of times TCP connections have made a direct
transition to the CLOSED state from either the SYN-SENT
state or the SYN-RCVD state, plus the number of times TCP
connections have made a direct transition to the LISTEN
state from the SYN-RCVD state.
(attemptFails)
"
    ::= { sonusGlobalTcpGeneralGroupCurrentStatisticsEntry 5 }

-- tagpath /global/tcpGeneralGroupCurrentStatistics/inSegments
sonusGlobalTcpGeneralGroupCurrentStatisticsInSegments OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The total number of segments received, including those
received in error.  This count includes segments received on
currently established connections.
"
    ::= { sonusGlobalTcpGeneralGroupCurrentStatisticsEntry 6 }

-- tagpath /global/tcpGeneralGroupCurrentStatistics/outSegments
sonusGlobalTcpGeneralGroupCurrentStatisticsOutSegments OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The total number of segments sent, including those on
current connections but excluding those containing only
retransmitted octets.
"
    ::= { sonusGlobalTcpGeneralGroupCurrentStatisticsEntry 7 }

-- tagpath /global/tcpGeneralGroupCurrentStatistics/retransSegments
sonusGlobalTcpGeneralGroupCurrentStatisticsRetransSegments OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "
The total number of segments retransmitted - that is, the
number of TCP segments transmitted containing one or more
previously transmitted octets.
"
    ::= { sonusGlobalTcpGeneralGroupCurrentStatisticsEntry 8 }

