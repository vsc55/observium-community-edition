        sonusGlobalPolicyServerServiceUsageCountersTgBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersTgForcedRouting Counter32,
        sonusGlobalPolicyServerServiceUsageCountersTgHiFraudBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersTgSacNonSacRoutes Counter32,
        sonusGlobalPolicyServerServiceUsageCountersCasualCallUnallowed Counter32,
        sonusGlobalPolicyServerServiceUsageCountersNonSubscriberCallsUnallowed Counter32,
        sonusGlobalPolicyServerServiceUsageCountersGwScreening Counter32,
        sonusGlobalPolicyServerServiceUsageCountersGwBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersGwForcedRouting Counter32,
        sonusGlobalPolicyServerServiceUsageCountersGwHiFraudBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersGwSacNonSacRoutes Counter32,
        sonusGlobalPolicyServerServiceUsageCountersAuthcodeScreening Counter32,
        sonusGlobalPolicyServerServiceUsageCountersAuthcodeBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersAuthcodeForcedRouting Counter32,
        sonusGlobalPolicyServerServiceUsageCountersAuthcodeHiFraudBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersAuthcodeSacNonSacRoutes Counter32,
        sonusGlobalPolicyServerServiceUsageCountersInfodigitScreening Counter32,
        sonusGlobalPolicyServerServiceUsageCountersInfodigitBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersInfodigitForcedRouting Counter32,
        sonusGlobalPolicyServerServiceUsageCountersInfodigitHiFraudBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersInfodigitSacNonSacRoutes Counter32,
        sonusGlobalPolicyServerServiceUsageCountersSoftswitchScreening Counter32,
        sonusGlobalPolicyServerServiceUsageCountersSoftswitchBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersSoftswitchForcedRouting Counter32,
        sonusGlobalPolicyServerServiceUsageCountersSoftswitchHiFraudBlocking Counter32,
        sonusGlobalPolicyServerServiceUsageCountersSoftswitchSacNonSacRoutes Counter32
    }

