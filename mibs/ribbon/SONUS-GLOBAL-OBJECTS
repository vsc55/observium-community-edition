"
    ::= { sonusGlobalMIB 111 }

-- tagpath /global/callResourceDetailStatus
sonusGlobalCallResourceDetailStatusEntry OBJECT-TYPE
    SYNTAX      SonusGlobalCallResourceDetailStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION ""
    INDEX { sonusGlobalCallResourceDetailStatusGCID, sonusGlobalCallResourceDetailStatusResIndex }
        ::= { sonusGlobalCallResourceDetailStatusTable 1 }

SonusGlobalCallResourceDetailStatusEntry ::=
    SEQUENCE {
        sonusGlobalCallResourceDetailStatusGCID Unsigned32,
        sonusGlobalCallResourceDetailStatusResIndex Unsigned32,
        sonusGlobalCallResourceDetailStatusResId Integer32,
        sonusGlobalCallResourceDetailStatusResType SonusCallResTypeType,
        sonusGlobalCallResourceDetailStatusCallId Integer32,
        sonusGlobalCallResourceDetailStatusLegId Integer32,
        sonusGlobalCallResourceDetailStatusNodeType SonusNodeType,
        sonusGlobalCallResourceDetailStatusS-Gcid Unsigned32,
        sonusGlobalCallResourceDetailStatusM-Gcid Unsigned32,
        sonusGlobalCallResourceDetailStatusT-Gcid Unsigned32,
        sonusGlobalCallResourceDetailStatusS-nodeIp String,
        sonusGlobalCallResourceDetailStatusM-nodeIp String,
        sonusGlobalCallResourceDetailStatusT-nodeIp String,
        sonusGlobalCallResourceDetailStatusNodeGcidAndIpAddr String
    }

