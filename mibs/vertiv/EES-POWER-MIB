        SYNTAX INTEGER  {
	    disabled(0),
	    rect-solar(1),
	    solar(2)
	}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION "
            Solar mode, 
		Disabled(0),
		RECT-SOLAR(1),
		Solar(2)"
	::= { psSolar 8 }

